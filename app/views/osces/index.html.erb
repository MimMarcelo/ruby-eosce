<p id="notice"><%= notice %></p>

<h1>Osces</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Resume</th>
      <th>Accept comments</th>
      <th>Fixed station order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @osces.each do |osce| %>
      <tr>
        <td><%= osce.title %></td>
        <td><%= osce.resume %></td>
        <td><%= osce.accept_comments %></td>
        <td><%= osce.fixed_station_order %></td>
        <td><%= link_to 'Show', osce %></td>
        <td><%= link_to 'Edit', edit_osce_path(osce) %></td>
        <td><%= link_to 'Destroy', osce, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Osce', new_osce_path %>
