<p id="notice"><%= notice %></p>

<h2><%= t("stations_template") %></h2>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th scope="col"><%= t("title") %></th>
      <th scope="col"><%= t("resume") %></th>
      <th scope="col"><%= t("procedure") %></th>
      <th scope="col"><%= t("evaluation") %></th>
      <th scope="col"><%= t("score") %></th>
      <th scope="col"><%= t("minutes") %></th>
      <th scope="col"><%= t("level") %></th>
      <th scope="col"><%= t("show") %></th>
      <th scope="col"><%= t("edit") %></th>
      <th scope="col"><%= t("destroy") %></th>
    </tr>
  </thead>

  <tbody>
    <% @station_templates.each do |station_template| %>
      <tr>
        <td><%= station_template.name %></td>
        <td><%= station_template.resume %></td>
        <td><%= station_template.procedure %></td>
        <td><%= station_template.evaluation %></td>
        <td><%= station_template.score %></td>
        <td><%= station_template.minutes %></td>
        <td><%= station_template.level %></td>
        <td><%= link_to 'info', station_template, class: "material-icons" %></td>
        <td><%= link_to 'edit', edit_station_template_path(station_template), class: "material-icons" %></td>
        <td><%= link_to 'delete', station_template, class: "material-icons", method: :delete, data: { confirm: t("are_you_sure") } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station Template', new_station_template_path %>